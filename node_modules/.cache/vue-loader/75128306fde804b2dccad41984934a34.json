iv class=\"row mt-3\">\n              <div class=\"col-12\">\n                <label>Select Book</label>\n                <select class=\"w-100 px-1 py-1\" v-model=\"selectedBook\">\n                  <option :value=\"''\" disabled selected>Please Select</option>\n                  <option\n                    v-for=\"(item,index) in books\"\n                    :value=\"item\"\n                    :key=\"index\"\n                  >{{ item.bookName }}</option>\n                </select>\n                <div class=\"resultArea\">\n                  <p>{{ userName }}</p>\n                  <p>{{ userAdress }}</p>\n                </div>\n              </div>\n            </div>\n            <div class=\"row mt-3\">\n              <div class=\"col-12\">\n                <button type=\"submit\" class=\"btn-secondary mx-auto d-block w-50\">Get from User</button>\n                <button type=\"button\" @click=\"showUser\" class=\"btn btn-primary\">Show User</button>\n              </div>\n              <div>\n                <li v-for=\"(item,index) in getUser\" :key=\"index\">\n                  {{ item.name }}\n                </li>\n              </div>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n<script>\nimport { mapGetters } from 'vuex'\nexport default {\n  name: \"GetBook\",\n  data() {\n    return {\n      selectedBook: \"\",\n      books: this.$store.state.books.filter((p) => p.inStore == false),\n      userName:\"\",\n      userAdress:\"\"\n    };\n  },\n  computed: {\n    ...mapGetters(['getUser']),\n  },\n  methods: {\n    showUser() {\nconsole.log(this.userName)\n      // console.log(this.selectedBook.bookName)\n    },\n    mounted(){\n      const index = this.getUser.findIndex(u=>u.selectedBook===this.selectedBook.bookName)\n      this.userName=this.getUser[index].name\n    this.userAdress=this.getUser[index].address\n    console.log(this.userName)\n    }\n  },\n};\n</script>\n\n<style>\n.resultArea {\n  margin-top: 5vh;\n  height: 10em;\n  background-color: lightcyan;\n}\n</style>"]}]}