nter mt-3\">\n      <div :class=\"inStore\" :style=\"inStore?inStoreStyle:atUserStyle\" class=\"status\">\n        <b>{{ inStore? 'In Store':'At User' }}</b>\n      </div>\n      <a v-if=\"inStore\" @click=\"confirmation\" class=\"mt-2\" style=\"color:red\">Delete</a>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex'\n\nexport default {\n  name: \"bookListItem\",\n  data() {\n    return {\n      inStoreStyle: {\n        color: \"green\",\n        background: \"hsl(120, 60%, 70%)\",\n        border: \"3px solid green\",\n      },\n      atUserStyle: {\n        color: \"red\",\n        background: \"rgba(255,0,0,0.3)\",\n        border: \"3px solid red\",\n      },\n    };\n  },\n  methods: {\n    ...mapActions({\n      delete:'deleteBook'\n    }),\n    confirmation() {\n      // console.log(this.bookId)\n      if (confirm(`Are you sure you want to delete the ${this.bookName}`)) {\n        this.delete(this.bookId)\n      }else {\n        return false;\n      }\n      \n    },\n  },\n  props: [\"bookId\",\"inStore\", \"bookName\", \"author\", \"imgUrl\"],\n};\n</script>\n\n<style>\n.status {\n  width: 150px;\n  text-align: center;\n  padding-left: 5px;\n  padding-right: 5px;\n}\n</style>"]}]}