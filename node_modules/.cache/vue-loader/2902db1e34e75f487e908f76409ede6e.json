itForm)\">\n              <div class=\"row mt-3\">\n                <div class=\"col-12 mt-3\">\n                  <label>Book Name</label>\n                  <ValidationProvider mode=\"passive\" rules=\"required\" v-slot=\"{ errors }\">\n                    <input type=\"text\" class=\"w-100 h-75 rounded-pill\" placeholder=\"  Please enter book name\" v-model=\"bookName\" />\n                    <span class=\"text-danger\">{{ errors[0] }}</span>\n                  </ValidationProvider>\n                </div>\n              </div>\n              <div class=\"row mt-3\">\n                <div class=\"col-12 mt-3\">\n                  <label>Author</label>\n                  <ValidationProvider mode=\"passive\" rules=\"required\" v-slot=\"{ errors }\">\n                    <input type=\"text\" class=\"w-100  h-75 rounded-pill\" placeholder=\"  Please enter author\" v-model=\"author\" />\n                    <span class=\"text-danger\">{{ errors[0] }}</span>\n                  </ValidationProvider>\n                </div>\n              </div>\n              <div class=\"row mt-3\">\n                <div class=\"col-12 mt-3\">\n                  <label>Image</label>\n                  <ValidationProvider mode=\"passive\" rules=\"required\" v-slot=\"{ errors }\">\n                    <input type=\"text\" class=\"w-100 h-75 rounded-pill\" placeholder=\"  Book Input URL\" v-model=\"imageUrl\" />\n                    <span class=\"text-danger\">{{ errors[0] }}</span>\n                  </ValidationProvider>\n                </div>\n              </div>\n              <div class=\"row mt-3\">\n                <div class=\"col-12\">\n                  <button type=\"submit\" class=\"btnStyle m-3 mt-4 h-75 rounded-pill mx-auto d-block w-50\">Save</button>\n                </div>\n              </div>\n            </form>\n          </ValidationObserver>\n        </div>\n      </div>\n    </div>\n  </div>\n\n</div>\n</template>\n\n<script>\nimport { ValidationProvider, ValidationObserver } from \"vee-validate\";\nexport default {\n  name: \"formComp\",\n  data() {\n    return {\n      id: new Date().getTime(),\n      bookName: \"\",\n      author: \"\",\n      imageUrl: \"\",\n      inStore: true,\n    };\n  },\n  components: {\n    ValidationProvider,\n    ValidationObserver,\n  },\n  methods: {\n    submitForm() {\n      const formAddingData = {\n        bookId: this.id,\n        bookName: this.bookName,\n        author: this.author,\n        imageUrl: this.imageUrl,\n        inStore: this.inStore,\n      };\n      this.$emit(\"onAddSubmit\", formAddingData);\n      this.clearForm();\n    },\n    clearForm() {\n      this.bookName = \"\";\n      this.author = \"\";\n      this.imageUrl = \"\";\n    },\n  },\n};\n</script>\n<style  scoped>\na {\n  color: brown;\n}\na:hover {\n  color: whitesmoke;\n}\n</style>\n\n<style>\n.addStyle {\n  background-color: #c7b198;\n  border-radius: 5px;\n\n}\n#backgroundBookList {\n   height: 100vh;\n  background-image: url(\"../assets/bookStore-background3.jpg\");\n  background-size: 100% 100%;\n  background-color: rgba(238, 238, 238, 0.3) !important;\n  background-blend-mode: color;\n}\n</style>"]}]}