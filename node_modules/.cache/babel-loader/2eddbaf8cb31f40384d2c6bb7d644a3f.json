     console.log(this.getAvailableBooks);\n    },\n    clearForm() {\n      this.selectedBook = \"\";\n      this.userId = \"\";\n      this.userName = \"\";\n      this.addressArea = \"\";\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}